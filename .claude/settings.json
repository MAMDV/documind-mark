{
  "name": "DocuMind Student Project",
  "version": "1.0.0",
  "description": "AI-Powered Knowledge Management System - Student Configuration",
  "agents": {
    "enabled": true,
    "defaultModel": "claude-3-5-sonnet-20241022"
  },
  "skills": {
    "enabled": true,
    "directory": ".claude/skills"
  },
  "subagents": {
    "enabled": true,
    "directory": ".claude/subagents"
  },
  "hooks": {
    "enabled": true,
    "directory": ".claude/hooks",
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/SessionStart.sh"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/UserPromptSubmit.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/Stop.sh"
          }
        ]
      }
    ]
  }
}
